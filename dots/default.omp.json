{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "blocks": [
    {
      "type": "rprompt",
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#828282",
          "template": " <#fefefe>\ufbab</> {{ .FormattedMs }} ",
          "properties": {
            "style": "austin",
            "threshold": 10000
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#424242",
          "template": " {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }}{{ .Staging.String }}{{ end }}  ",
          "properties": {
            "branch_icon": "\ue0a0 ",
            "fetch_stash_count": false,
            "fetch_status": true,
            "ignore_status": [
              "internal"
            ]
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "foreground": "#ffffff",
          "background": "#eec10f",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} ",
          "properties": {
            "display_mode": "environment",
            "fetch_version": false
          }
        },
        {
          "type": "kubectl",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "#0959fd",
          "template": " \ufd31 {{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}} "
        },
        {
          "type": "terraform",
          "style": "powerline",
          "foreground": "#000000",
          "background": "#ebcc34",
          "template": "{{.WorkspaceName}} "
        },
        {
          "type": "gcp",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#47888d",
          "template": "  {{.Project}} :: {{.Account}} "
        },
        {
          "type": "aws",
          "style": "powerline",
          "foreground": "#ffffff",
          "background": "#FFA400",
          "template": "  {{.Profile}}{{if .Region}}@{{.Region}}{{end}} "
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "foreground": "#56B6C2",
          "background": "#2f2f2f",
          "trailing_diamond": "\ue0b0 ",
          "template": " \ue5ff {{ .Path }} ",
          "properties": {
            "style": "full"
          }
        }
      ],
      "newline": true
    }
  ]
}
